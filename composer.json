{
    "name": "tecnoparque/gestion2021",
    "description": "SIRT es un sistema de información para la gestion de los procesos internos de la red Tecnoparque Colombia, basado en Laravel",
    "keywords": [
        "framework",
        "Laravel",
        "Sistema",
        "SIRT",
        "SENA"
    ],
    "authors": [
        {
            "name": "Julian Londoño Raigosa",
            "email": "jlondono433@gmail.com"
        },
        {
            "name": "Victor Manuel Moreno Vega",
            "email": "vmmorenov@sena.edu.co"
        }
    ],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "doctrine/dbal": "^3.0",
        "laravel/framework": "^10.0",
        "laravel/tinker": "^2.8.0",
        "laravel/ui": "^4.2",
        "maatwebsite/excel": "^3.1",
        "realrashid/sweet-alert": "^7.1.0",
        "spatie/laravel-permission": "^5.8",
        "tymon/jwt-auth": "^2.0.0",
        "yajra/laravel-datatables": "10.0"
    },
    "require-dev": {
        "filp/whoops": "^2.15.2",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.6.5",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Repositories\\": "app/Repositories"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files":["app/Http/Helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
